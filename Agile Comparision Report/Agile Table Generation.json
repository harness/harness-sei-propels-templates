                                "value": "prefix = 'hello ';\nfunction handleNode(context) {\n  var columns = [\"Sprint\",\"Date\",\"ou_id\",\"OU Name\",\"Stories Completed\",\"Sprint Hygiene\",\"Commit to Done\",\"Scope Creep\",\"Sprint Velocity\",\"Story Point\",\"Total Ticket\",\"Idle\",\"Missing Story points\",\"Large Stories\",\"AC Missing\",\"Poor Description\",\"No Assignee\",\"Creep Story Points\",\"Creep Stories\"];\n\n\nfunction create_UUID(){\n    var dt = new Date().getTime();\n    var uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {\n        var r = (dt + Math.random()*16)%16 | 0;\n        dt = Math.floor(dt/16);\n        return (c=='x' ? r :(r&0x3|0x8)).toString(16);\n    });\n    return uuid;\n}\nvar uuid  = create_UUID();\nvar payload = {\n    name: \"Agile Comparison Report\",\n    rows: {},\n    schema: {}\n}\n\npayload.rows[uuid] = {id: uuid,index:0};\npayload.schema.columns = {};\ncolumns.forEach((element, index) => {\n    payload.rows[uuid][index] = '';\n    payload.schema.columns[index] = {};\n    var columnkey = index + 1;\n    payload.schema.columns[index][\"id\"] = index;\n    payload.schema.columns[index][\"index\"] = index + 1;\n    payload.schema.columns[index][\"key\"] = \"column_\" + columnkey;\n    payload.schema.columns[index][\"display_name\"] = element;\n    payload.schema.columns[index][\"type\"] = element == \"Date\" ? \"date\": \"string\";\n    payload.schema.columns[index][\"options\"] = [];\n});\n\n\nconsole.log(payload);\n  // levelops.serviceName().doSomething() - invoke a levelops service\n  return { \n    state : 'success',\n    output : { \n      payload \n    } \n  };\n}",
                                "type": "string"
