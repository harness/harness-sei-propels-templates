                        "values": [
                            {
                                "value": "prefix = 'hello ';\nfunction handleNode(context) {\n  \n  function create_UUID(){\n    var dt = new Date().getTime();\n    var uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {\n        var r = (dt + Math.random()*16)%16 | 0;\n        dt = Math.floor(dt/16);\n        return (c=='x' ? r :(r&0x3|0x8)).toString(16);\n    });\n    return uuid;\n}\n\n//console.log(create_UUID());\n  \n  var dashboard = JSON.parse(context.getParam('dashboard'));\n  dashboard.widgets.forEach(function(widget){\n  \twidget.id = create_UUID();\n  \n  });\n  \n  \n  dashboard.category = [\"96bdb70e-8bdb-4c05-9963-1181178e7343\"];\n  \n  delete dashboard.id;\n  \n  // levelops.serviceName().doSomething() - invoke a levelops service\n  return { \n    state : 'success',\n    output : { \n      'dashboard' : dashboard\n    } \n  };\n}",
                                "type": "string"
                            }
